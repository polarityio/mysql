{{#each block.data.details as | row index |}}
  {{#if (gt index 0)}}
    <hr>
  {{/if}}
  <h1 class="p-title">Result #{{index}}</h1>
  {{#each-in row as |key value|}}
    <div>
      <span class="p-key">{{key}}:</span>
      <span class="p-value">{{value}}</span>
    </div>
  {{/each-in}}
{{/each}}

